  "description": "Selenium-WebDriver end-to-end test automation boilerplate. Using TypeScript, ts-node and Tape.",
  "devDependencies": {
    "@types/node": "13.9.2",
    "@types/selenium-webdriver": "4.0.9",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "prettier": "2.0.2",
    "tslint": "6.1.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.2.0"
  },
  "homepage": "https://github.com/e2e-boilerplate/selenium-webdriver-typescript-ts-node-tape#readme",
  "husky": {
